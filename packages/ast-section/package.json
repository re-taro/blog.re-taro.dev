{
	"name": "ast-section",
	"version": "2.7.0",
	"type": "module",
	"private": true,
	"exports": {
		".": {
			"default": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
