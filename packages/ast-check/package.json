{
	"name": "ast-check",
	"version": "2.7.0",
	"type": "module",
	"private": true,
	"exports": {
		".": {
			"default": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsc -p . --incremental false",
		"format": "prettier --write .",
		"lint": "pnpm run /^lint:.*/",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check .",
		"lint:tsc": "tsc -p . --noEmit"
	},
	"dependencies": {
		"ast": "workspace:*"
	}
}
