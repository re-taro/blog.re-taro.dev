{
	"name": "remark-embed",
	"version": "3.0.0",
	"type": "module",
	"private": true,
	"exports": {
		".": {
			"default": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "rollup -c",
		"build:dev": "pnpm run build",
		"format": "prettier --write .",
		"lint": "pnpm run /^lint:.*/",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check .",
		"lint:tsc": "tsc -p . --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@types/mdast": "catalog:",
		"ast": "workspace:*",
		"mdast-util-from-markdown": "catalog:",
		"remark-check": "workspace:*",
		"unified": "catalog:",
		"visit": "workspace:*"
	},
	"devDependencies": {
		"remark-gfm": "4.0.0",
		"remark-parse": "11.0.0",
		"rollup-config": "workspace:*"
	}
}
