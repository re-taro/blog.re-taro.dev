{
	"name": "rollup-config",
	"version": "3.0.0",
	"type": "module",
	"private": true,
	"exports": {
		".": {
			"default": {
				"types": "./index.d.ts",
				"default": "./index.js"
			}
		}
	},
	"files": [
		"index.d.ts",
		"index.js"
	],
	"scripts": {
		"format": "wireit",
		"lint": "wireit",
		"lint:eslint": "wireit",
		"lint:prettier": "wireit"
	},
	"dependencies": {
		"@rollup/plugin-commonjs": "28.0.6",
		"@rollup/plugin-json": "6.1.0",
		"@rollup/plugin-node-resolve": "16.0.1",
		"@rollup/plugin-replace": "6.0.2",
		"@rollup/plugin-terser": "0.4.4",
		"@rollup/plugin-typescript": "12.1.4",
		"rollup-plugin-output-size": "1.6.1"
	},
	"wireit": {
		"format": {
			"command": "prettier --write ."
		},
		"lint": {
			"dependencies": [
				"lint:eslint",
				"lint:prettier"
			]
		},
		"lint:eslint": {
			"command": "eslint ."
		},
		"lint:prettier": {
			"command": "prettier --check ."
		}
	}
}
