{
	"name": "@redwoodjs/starter-minimal",
	"version": "1.0.0",
	"type": "module",
	"description": "A bare-bones RedwoodSDK starter",
	"author": "",
	"license": "MIT",
	"private": true,
	"main": "index.js",
	"keywords": [],
	"scripts": {
		"build": "NODE_ENV=${NODE_ENV:-production} vite build",
		"dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
		"dev:init": "rw-scripts dev-init",
		"format": "prettier --write .",
		"generate": "rw-scripts ensure-env && wrangler types && panda codegen --silent",
		"lint": "pnpm run /^lint:.*/",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check .",
		"lint:textlint": "textlint contents/**/slug.md",
		"lint:tsc": "pnpm run generate && tsc -p .",
		"prebuild": "panda codegen --silent",
		"preview": "pnpm run build && vite preview",
		"scaffold": "plop",
		"start": "wrangler dev",
		"test:integration": "playwright test",
		"worker:run": "rw-scripts worker-run"
	},
	"dependencies": {
		"@redwoodjs/sdk": "0.0.64",
		"ast": "workspace:*",
		"dompurify": "3.2.5",
		"hast-util-from-html": "2.0.3",
		"hast-util-select": "6.0.4",
		"hast-util-to-html": "9.0.5",
		"react": "canary",
		"react-dom": "canary",
		"react-server-dom-webpack": "canary",
		"temporal-polyfill": "0.3.0",
		"xml2js": "0.6.2"
	},
	"devDependencies": {
		"@axe-core/playwright": "4.10.1",
		"@content-collections/core": "0.8.0",
		"@content-collections/vite": "0.2.4",
		"@pandacss/dev": "0.53.3",
		"@playwright/test": "1.49.1",
		"@qwik.dev/partytown": "0.11.0",
		"@shikijs/transformers": "3.2.1",
		"@types/hast": "3.0.4",
		"@types/react": "19.1.1",
		"@types/react-dom": "19.1.2",
		"@types/xml2js": "0.4.14",
		"ast-article": "workspace:*",
		"ast-description-list": "workspace:*",
		"ast-embed": "workspace:*",
		"ast-section": "workspace:*",
		"ast-toc": "workspace:*",
		"ast-transform": "workspace:*",
		"axe-core": "4.10.3",
		"axe-playwright": "2.0.3",
		"oembed": "workspace:*",
		"playwright": "1.49.1",
		"plop": "4.0.1",
		"postcss": "8.5.1",
		"remark-description-list": "workspace:*",
		"remark-embed": "workspace:*",
		"remark-gfm": "4.0.0",
		"remark-parse": "11.0.0",
		"remark-retext": "6.0.0",
		"retext-english": "5.0.0",
		"retext-equality": "7.1.0",
		"retext-stringify": "4.0.0",
		"schema-dts": "1.1.5",
		"sharp": "0.34.1",
		"shiki": "3.2.2",
		"sirv-cli": "3.0.0",
		"textlint": "14.4.2",
		"textlint-filter-rule-comments": "1.2.2",
		"textlint-rule-ja-space-around-link": "2.4.2",
		"textlint-rule-preset-overflow-techblog": "0.0.2",
		"ulid": "3.0.0",
		"unfurl.js": "6.4.0",
		"unified": "11.0.5",
		"vite": "6.2.5",
		"vitest": "3.1.1",
		"wrangler": "4.10.0"
	}
}
